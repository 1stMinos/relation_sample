<p id="notice"><%= notice %></p>

<h1>Listing From Station Ways</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @from_station_ways.each do |from_station_way| %>
      <tr>
        <td><%= from_station_way.name %></td>
        <td><%= link_to 'Show', from_station_way %></td>
        <td><%= link_to 'Edit', edit_from_station_way_path(from_station_way) %></td>
        <td><%= link_to 'Destroy', from_station_way, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

