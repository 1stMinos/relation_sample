<h1>Listing Offices</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Stations</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offices.each do |office| %>
      <tr>
        <td><%= office.name %></td>
        <td><% unless office.from_stations.blank? %>
            <% p office %>
            <% p office.from_stations %>
            <%ã€€hoge = office.from_stations[0] %>
            <% p hoge %>
            <% end %></td>
        <td><%= office.latitude %></td>
        <td><%= office.longitude %></td>
        <td><%= office.comment %></td>
        <td><%= link_to 'Show', office %></td>
        <td><%= link_to 'Edit', edit_office_path(office) %></td>
        <td><%= link_to 'Destroy', office, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

